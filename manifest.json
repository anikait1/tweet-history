{
  "manifest_version": 2,
  "version": "1.0",
  "name": "Tweet History",
  "content_scripts": [
    {
      "matches": ["*://*.twitter.com/*"],
      "js": ["index.js"]
    }
  ],
  "browser_action": {
    "default_title": "Tweet History",
    "default_popup": "index.html"
  },
  "icons": {},
  "permissions": ["activeTab", "storage"]
}
